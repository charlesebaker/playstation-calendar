{
  "name": "playstation-calendar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install-and-run": "yarn install && vite",
    "dev": "vite",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --ext js,jsx,ts,tsx,css,md,json --write",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ],
    "**/*.{js,jsx,ts,tsx,html,css,md,json}": [
      "yarn format"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@heroicons/react": "^2.1.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "date-fns": "^3.3.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "path": "^0.12.7",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-router-dom": "^6.22.0",
    "react-toastify": "^10.0.4",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
