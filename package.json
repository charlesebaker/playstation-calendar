{
  "name": "playstation-calendar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --ext js,jsx,ts,tsx,css,md,json --write",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ],
    "**/*.{js,jsx,ts,tsx,html,css,md,json}": [
      "yarn format"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "date-fns": "^3.3.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "path": "^0.12.7",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "react-router-dom": "^6.22.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
